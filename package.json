{
  "name": "capbox",
  "description": "The brand new toolbox for Capacitor.io tasks automation.",
  "version": "2.1.0",
  "private": false,
  "bin": {
    "capbox": "./bin/capbox"
  },
  "keywords": [
    "plugins",
    "capacitor",
    "universal app",
    "progressive web apps",
    "electron",
    "pwa",
    "desktop",
    "android",
    "universal",
    "ios",
    "ionic",
    "cli",
    "tool"
  ],
  "scripts": {
    "babel": "babel",
    "build": "babel src/ -d dist --copy-files --include-dotfiles",
    "start": "cross-env CAPBOX_STAGE=development node src/index",
    "capbox": "cross-env CAPBOX_STAGE=development node src/capbox",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.7.4",
    "@capacitor/cli": "^1.3.0",
    "babel-eslint": "^10.0.3",
    "brotli": "^1.3.2",
    "clean-css": "^4.2.1",
    "colors": "^1.4.0",
    "commander": "^4.0.1",
    "cross-env": "^6.0.3",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "ip": "^1.1.5",
    "ngzip": "^1.1.3",
    "path": "^0.12.7",
    "shelljs": "^0.8.3",
    "uglify-js": "^3.7.1"
  },
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 Chrome versions"
          ]
        }
      }
    ]
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/runtime": "^7.11.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "license": "MIT",
  "author": {
    "name": "Leonardo Quevedo",
    "email": "lpachecoquevedo@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/leopq/capbox/issues"
  },
  "homepage": "https://github.com/leopq/capbox",
  "contributors": [
    {
      "name": "leopq",
      "email": "lpachecoquevedo@gmail.com"
    }
  ]
}
